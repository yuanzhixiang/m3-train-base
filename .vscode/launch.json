{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: minimal_gpt.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/minimal_gpt.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
